#include<stdio.h>  
#include<string.h> 
int main()  
{  
    char a[128],b[128];  
    int f=1,i;  
    fgets(a,1000,stdin);  
    fgets(b,1000,stdin);  
    for(i=0;a[i]!='\n';i++){  
        if(a[i]!=b[i]) f=0;  
    }  
    if(f==1) printf("YES\n");  
    else printf("NO\n");  
    return 0;   
} 
